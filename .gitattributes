*.gz filter=lfs diff=lfs merge=lfs
*.tar filter=lfs diff=lfs merge=lfs
*.zip filter=lfs diff=lfs merge=lfs
*.rar filter=lfs diff=lfs merge=lfs

# Video
*.mkv filter=lfs diff=lfs merge=lfs
*.avi filter=lfs diff=lfs merge=lfs
*.mov filter=lfs diff=lfs merge=lfs
*.mp4 filter=lfs diff=lfs merge=lfs
*.flv filter=lfs diff=lfs merge=lfs
*.mpg filter=lfs diff=lfs merge=lfs
*.mpeg filter=lfs diff=lfs merge=lfs
*.vob filter=lfs diff=lfs merge=lfs
*.amv filter=lfs diff=lfs merge=lfs
*.3gp filter=lfs diff=lfs merge=lfs

# Disk images
*.iso filter=lfs diff=lfs merge=lfs
*.elf filter=lfs diff=lfs merge=lfs

# Exe
*.exe filter=lfs diff=lfs merge=lfs
